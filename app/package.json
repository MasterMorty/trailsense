{
  "name": "app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "generate-migrations": "npx drizzle-kit generate --config=./db/drizzle.config.ts",
    "apply-migrations:dev": "npx wrangler d1 migrations apply DB",
    "apply-migrations:prod": "npx wrangler d1 migrations apply --remote",
    "apply-seeds:dev": "npx wrangler d1 execute DB --file=./db/seeds/demo.sql",
    "apply-seeds:prod": "npx wrangler d1 execute --remote DB --file=./db/seeds/demo.sql"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^7.0.0-beta.1",
    "@vueuse/core": "^14.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "culori": "^4.0.2",
    "drizzle-orm": "^0.44.7",
    "lucide-vue-next": "^0.553.0",
    "maplibre-gl": "^5.11.0",
    "nuxt": "^4.2.1",
    "nuxt-maplibre": "^1.2.0",
    "pmtiles": "^4.3.0",
    "shadcn-nuxt": "^2.3.2",
    "strava-v3": "^2.2.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.6",
    "nitro-cloudflare-dev": "^0.2.2",
    "wrangler": "^4.46.0"
  }
}
